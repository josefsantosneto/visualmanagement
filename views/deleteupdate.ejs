<%-include("partials/header");-%>

<div class="container mw-100 mh-100">
            <div class="row rep"></div>
            <div class="row">
                
            </div>
            <div class="row">
                <div class="col-2 text-white">Marcar</div>
                <div class="col-2 text-white">Nome do Projeto</div>
                <div class="col-2 text-white">Descrição</div>
                <div class="col-2 text-white">Setor Atual</div>               
                <div class="col-2 text-white">Valor</div>
                <div class="col-2 text-white">Atualizado por</div>
                


            </div>
    <form action="/deleteupdate" method="POST">
        <% projetos.forEach(function(projeto){ %>
            <div class="row">
                <div class="col-2"><input class="rep-field" type="checkbox" name="checkbox" value="<%=projeto._id%>"></div>
                <div class="col-2"><input class="rep-field" disabled name="nomeprojeto" type="text" value="<%=projeto.nomeDoProjeto%>"></div>
                <div class="col-2"><input class="rep-field" disabled name="descricao" type="text" value="<%=projeto.descricao%>"></div>
                <div class="col-2"><input class="rep-field"  disabled name="setorAtual" type="text" value="<%=projeto.setorAtual%>"></div>
                <div class="col-2"><input class="rep-field"  disabled name="valor" type="text" value="<%=projeto.valor%>"></div>
                <div class="col-2"><input class="rep-field"  disabled name="ultimaAtualizacao" disabled="false" type="text" value="<%=projeto.ultimaAtualizacao[0].username%>"></div>
                <input type="hidden" name="id2" value="<%=projeto._id%>">
    
            </div>
        <% }); %>
        

    
        <div class="text-center rep"></div>
        <div class="text-center"><button type="submit">Apagar selecionados</button></div>
    </form>       
    <div class="row rep"></div>
    <div class="text-center">
        <div class="">
           <!-- <select name="tipoOperacao" class="text-center">
                <option value="atualizar">Atualizar</option>
                <option value="excluir">Excluir</option>
            </select> -->
        </div>
        
    </div>

    <div class="row">
        <a class="a" href="/menuadm"> Voltar para Menu</a>
    </div>

    
</div>

<%-include("partials/footer");-%>